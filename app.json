{
  "expo": {
    "name": "fipe-app",
    "slug": "fipe-app",
    "scheme": "fipe-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.edubarreira1212.fipeapp"
    },
    "android": {
      "package": "com.edubarreira1212.fipeapp",
      "splash": {
        "mdpi": "./assets/splash.png",
        "backgroundColor": "#0A0A0A",
        "resizeMode": "contain"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "83b55edf-8eed-4a0d-8d48-bb07cc0c4617"
      }
    }
  }
}
